page{
  height: 100%;
}
.all{
  width: 95%;
  margin: auto;
  height: 90%;
}
.title{
  padding: 10rpx 15rpx;
  font-size: 33rpx;
}
.textarea{
  margin-top: 30rpx;
  padding: 0 15rpx;
  width: 100%;
  height: 600rpx;
}
.button{
  display: flex;
  flex-direction: row;
  margin-top: 65rpx;
  position: absolute;
  width: 95%;
  margin-bottom: 30rpx;
}
.but_images{
  width: 35rpx;
  height: 35rpx;
  margin-right: 10rpx;
}
.but_fbwt{
  flex: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background-color: #2896CC;
  color: #fff;
  padding: 20rpx 0;
  border-radius: 10rpx;
}
.images{
  margin-left: 20rpx;
  width: 210rpx;
  height: 210rpx;
   border-radius: 10rpx;
}
.upload{
  position: relative;
  display: inline-block;
}
.delect{
  position: absolute;
  top: -10rpx;
  left: 200rpx;
   width: 60rpx;
  height: 60rpx;
}
.del{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 0;
  bottom: 20rpx;
}