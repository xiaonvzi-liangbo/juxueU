/* pages/universityCircle/circle/circle.wxss */
.all{
  margin: 0 auto;
}
.search{
  width: 95%;
  margin: 0 auto;
  text-align: center;
  background-color: #EBEBEB;
  border-radius: 10rpx;
  color: #B5B5B5;
  padding: 5rpx 0;
  margin-top: 20rpx;
}
.title{
  font-weight: 550;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  margin-left: 25rpx;
  color: #252525;
}
.row{
  width: 95%;
  margin: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-shadow: 0 0 8rpx #DDDDDD;
  border-radius: 10rpx;
  margin-top: 20rpx;
}
.left_image{
  margin: 15rpx 10rpx 15rpx 25rpx;
}
.title1{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 10rpx;
  position: relative;
  top: -30rpx;
}
.conter{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  margin-left: 10rpx;
  position: relative;
  top: 20rpx;
}
.auto{
  flex: auto;
  text-align: center;
  margin-left: 10rpx;
}
.school_name{
  font-size: 36rpx;
  font-weight: 900;
}
.add{
  font-size: 25rpx;
  padding: 8rpx 30rpx;
  background-color: #2896CC;
  border-radius: 10rpx;
  color: #fff;
}
.finish_bot{
  font-size: 28rpx;
  text-align: center;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  color: #B5B5B5;
}