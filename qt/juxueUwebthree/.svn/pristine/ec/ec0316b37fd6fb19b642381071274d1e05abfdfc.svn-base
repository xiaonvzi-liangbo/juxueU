/* pages/home/home.wxss */

page {
  background-color: #fff;
}

.gun {
  position: relative;
  height: 100vh;
  overflow-x: hidden;
  overflow-y: auto;
}

/*隐藏滚动条*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}

.announce image {
  width: 96rpx;
  height: 96rpx;
  border: 6rpx #fff solid;
  margin-bottom: 24rpx;
  border-radius: 50%;
}

.sign {
  width: 100rpx;
  height: 100rpx;
  display: flex;
  position: relative;
  float: right;
  right: 50rpx;
  bottom: 250rpx;
  border-radius: 50%;
  box-shadow: 0rpx 1rpx 14rpx 2rpx #dbd9d9;
  z-index: 9999;
}
