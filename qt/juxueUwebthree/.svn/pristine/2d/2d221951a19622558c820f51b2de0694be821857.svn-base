.modal{
  border: 1rpx solid #ebebeb;
  height: 300rpx;
  width: 90%;
  box-shadow: 0 0 10rpx #ebebeb;
  font-size: 25rpx;
  text-align: center;
  background-color: #fff;
  top: 60rpx;
  border-radius: 10rpx;
  position:absolute;
}
.modalcov{
  height: 300rpx;
  width: 100%;
  font-size: 25rpx;
  text-align: center;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
   position:absolute;
   z-index: 9999 !important
}
.conter{
  position: relative;
}