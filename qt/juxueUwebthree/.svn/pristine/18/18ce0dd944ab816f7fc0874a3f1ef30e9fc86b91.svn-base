.page{
  height: 100%;
}
.card{
  width: 95%;
  margin: auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-radius: 0 12rpx 12rpx 0; 
  box-shadow: 0 0 10rpx #E4E4E4;
  margin-bottom: 20rpx;
}
.conter{
  width: 60%;
  text-align: left;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.con_left{
  padding: 5rpx 2rpx 10rpx 0rpx;
}
.con_title{
  font-size: 36rpx;
  font-weight: 550;
}
.con_time{
  font-size: 28rpx;
  color: #252525;
  margin: 30rpx 0;
}
.con_num{
  font-size: 25rpx;
  color: #A7A7A7;
}
.com_make{
  display: flex;
  flex-direction: column-reverse;
  position: absolute;
  right: 35rpx;
  margin-top: 135rpx;
  border-radius: 0 0 12rpx 0;
  background-color: #2896CC;
  color: #fff;
  font-size: 28rpx;
  padding: 10rpx 30rpx;
}
.swip{
  width: 100%;
  margin-top:25rpx;
  margin-right: 4rpx;
  margin-left: 4rpx;
  height:100%

}