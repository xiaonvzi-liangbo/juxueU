<navigation dome="个人认证中心"></navigation>
<view class="overall">
  <view style="margin:0 55rpx">
    <form bindsubmit="accessToken" data-fun="formSubmit">
      <view class="title">*姓名</view>

      <input placeholder="请填写姓名" placeholder-style="color:#ccc" class="input" name="name"></input>
      <view class="title">*学校</view>
      <view class="input">
        <pickers bind:traCheckedNum="checkNum"></pickers>
      </view>
      <view class="title">*专业</view>
      <input placeholder="请填写专业" placeholder-style="color:#ccc" class="input" name="zy"></input>
      <view class="title">*介绍</view>
      <input placeholder="介绍一下自己" placeholder-style="color:#ccc" class="input" name="text"></input>
      <button class="subim" formType="submit">提交</button>
    </form>
  </view>
</view>