scroll-view {
  width: 90%;
  margin: 0 auto 30rpx;
  font-size: 25rpx;
  line-height: 35rpx;
}

.header {
  font-size: 35rpx;
  text-align: center;
  margin: 18rpx 0;
  font-weight: 1000;
}
.introduction, .title_two ,.title_two_content{
  font-size: 30rpx;
  line-height: 40rpx;
}

.title_one {
  color: #474c48;
  font-weight: 800;
  margin: 10rpx 0;
  font-size: 35rpx;
}

.title_content, .title_two_content {
  margin-left: 25rpx;
}

.hollow {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  border: 2rpx #000 solid;
  position: absolute;
  left: 30rpx;
  margin-top: 10rpx;
}

.solid {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background-color: #000;
  border: 2rpx #000 solid;
  position: absolute;
  left: 35rpx;
  margin-top: 10rpx;
}
