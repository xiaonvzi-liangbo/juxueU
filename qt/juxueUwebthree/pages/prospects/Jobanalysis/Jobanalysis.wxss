page{
  overflow: hidden;
  height: 100%;
}
.banimg{
  width: 100%;
  height: 500rpx;
}
.title{
  text-align: center;
  position: absolute;
  top: 40px;
  left: 20px;
  color: #fff;
  z-index: 10;

} 
.analysis{
  background-color: #fff;
  border-radius: 40rpx 40rpx 0 0;
  position: relative;
  width: 100%;
  top: -40rpx;
  height: 100%;
}
.scroll{
  height: 44%;
}
.an_conter{
  padding: 0rpx 60rpx 40rpx 60rpx;
  display: flex;
  flex-direction: row;
}
.an_img{
  width: 120rpx;
  height: 120rpx;
}
.an_title{
  font-size: 36rpx;
  color: #2996CC;
  font-weight: 900;
  position: relative;
  top: 10rpx;
  left: 20rpx;
}
.an_text{
  font-size: 25rpx;
  position: relative;
  top: 20rpx;
  left: 20rpx;
  color: #868686;
}
.bottom{
  width: 100%;
  padding: 0rpx 60rpx 0rpx 60rpx;
  position: fixed;
  bottom: 0;
  left: 8rpx;
  background-color:#fff;
  box-shadow: 10rpx 10rpx 30rpx #fff;
}
.bot_but{
  width: 360rpx;
  border-radius: 50rpx;
  color: #fff;
  height: 86rpx;
  line-height: 86rpx;
  font-size: 33rpx;
  margin-top: 20rpx; background-image: linear-gradient(to bottom right,#7590f2, #2b6cef);
}
.bot_header
{
    height:72rpx;
    line-height:72rpx;
    text-align:center;
}
.bot_text{
  font-size: 22rpx;
  color: #828282;
  margin: 0 15rpx;
}
.bot_line
{
    display:inline-block;
    width:110rpx;
    border-top:1px solid #cccccc;
    vertical-align:6rpx;
}
.bot_img{
  width: 35rpx;
  height: 35rpx;
}
.bot_img_text{
  font-size: 25rpx;
  color: #4E4E4E;
  margin-left: 10rpx;
}
.bot_bottom{
  margin: 25rpx 0 30rpx 0;
  display: flex;
  justify-content: center;
}