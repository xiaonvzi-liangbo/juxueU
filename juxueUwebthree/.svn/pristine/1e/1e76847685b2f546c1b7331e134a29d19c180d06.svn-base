page{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.navigation{
  width: 70%;
  margin: auto;
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
}
.nav_text{
  text-align: center;
  flex:auto;
  font-size: 30rpx;
  color: #A7A7A7;
}
.checked{
  color: #2996CC;
  font-size: 33rpx;
  font-weight: 550;
}
.underline{
  margin-top: 30rpx;
  border-top:1rpx solid #D1D1D1;
}
.switch{
  height: 100%;
}
