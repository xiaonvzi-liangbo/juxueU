/* pages/prospects/welfare/welfare.wxss */
page{
  height: 100%;
  background-color: #A3D1F1;
}
.all{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.backimg{
  width: 100%;
  height: 100%;
}
.banht{
  width: 20px;
  height: 20px;
}
.fhan{
  position: absolute;
  top: 40px;
  left: 20px;
}
.fh_text{
  position: relative;
  left: 10px;
  top: -5px;
  color: #fff;
  font-size: 15px;
}
.content{
  background-color: #fff;
  border-radius: 20rpx;
  width: 95%;
  text-align: center;
  position: absolute;
  top: 30%;
  left: 20rpx;
  box-shadow: 0rpx 0rpx 10rpx #6EB3E0;
}
.con_head{
  margin: 60rpx 40rpx 0 40rpx;
}
.head_img{
  display: flex;
  margin: auto;
}
.head_invite{
  margin: auto;
  align-items: center;
  text-align: center;
}
.head_text{
  background-color: #287ACC;
  border-radius: 50rpx;
  padding: 5rpx 20rpx;
  font-size: 25rpx;
  color: #fff;
  float: left;
  position: absolute;
  left: 75rpx;
  top: 180rpx;
}
.circle{
  border-radius: 50%;
  border: 8rpx solid #287ACC;
}
.con_numb{
  margin-top: 70rpx;
  font-weight: 550;
  font-size: 33rpx;
}
.con_time{
  margin-top: 20rpx;
  margin-bottom: 50rpx;
  font-weight: 550;
  color: #909090;
  font-size: 33rpx;
}
.con_but{
  background-color: #287ACC;
  height: 90rpx;
  width: 80%;
  margin: auto;
  margin-bottom: 50rpx;
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.con_text{
  color: #fff;
  margin-left: 20rpx;
  font-size: 33rpx;
}
.activity{
  width: 100%;
  height: 100rpx;
  position: absolute;
  top: 73%;
}
.img_position{
  width: 70%;
  height: 100%;
  position: absolute;
  /* top: -80rpx; */
  top: 30rpx;
  left: 190rpx;
}
.bot_header
{
  height:72rpx;
  line-height:72rpx;
  text-align:center;
  position: absolute;
  top: 15rpx;
  left: 70rpx;
}
.bot_text{
  font-size: 33rpx;
  color: #fff;
  margin: 0 5rpx;
}
.bot_line
{
    display:inline-block;
    width:50rpx;
    border-top:1px solid #fff;
    vertical-align:10rpx;
}
.act_text{
  font-size: 25rpx;
  width: 85%;
  margin: 0 auto;
  margin-top: 100rpx;
  margin-bottom: 40rpx;
}
.act_cont{
  line-height: 40rpx;
  color: #496071;
}
.con_succes{
  margin: 90rpx 0;
  font-size: 33rpx;
  font-weight: 550;
}
.modal{
  width: 70%;
  border: 2rpx solid #BABABA;
  margin: 0 auto;
  padding: 10rpx;
  text-align: left;
  margin-top: 20rpx;
}
.modal_input{
  padding: 0rpx 10rpx;
}
.modal_xhx{
  width: 95%;
  border: 1rpx solid #DADADA;
  margin: auto;
}
.modal_text{
  width: 70%;
  margin: auto;
  color: #EC4C4C;
  text-align: left;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-size: 25rpx;
}
.model_but{
  width: 70%;
  height: 80rpx;
  font-size: 33rpx;
  border-radius: 50rpx;
  background-color: #287ACC;
  color: #fff;
}
.close{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 40rpx;
  top: 40rpx;
}