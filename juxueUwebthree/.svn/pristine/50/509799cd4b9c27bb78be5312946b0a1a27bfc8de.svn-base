// pages/prospects/welfare/welfare.js
Page({

  /**
   * 页面的初始数据
   */
  data: {
    show:true,
    cancel:false,
    showModel: false,
    show:false
  },
  //立即领取点击事件
  draw(){
    console.log('点击打开时间')
    this.setData({
      showModel:true
    })
  },
  //填写完信息立即领取点击事件
  close(){
    this.setData({
      showModel:false
    })
  },
  //邀请好友点击事件
  transmit(){
    this.setData({
      show: false
    })
  },
  //关闭模态框
  occlude(){
    this.setData({
      showModel: false
    })
  }
})