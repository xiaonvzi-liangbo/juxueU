page{
  height: 100%;
  background:url('https://staticfile.xlcwx.com/userfiles/201909/20190917104019.png') no-repeat;
  background-size:100%;
}
.navi{
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #fff;
  position: absolute;
  left: 30rpx;
  top: 70rpx;
  font-size: 17px;
}
.fhImage{
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
}
.conter{
  margin: auto;
  width: 90%;
  background-color: #fff;
  position: absolute;
  left: 38rpx;
  top: 33%;
  height: 65%;
  border-radius: 20rpx;
}
.bottom{
  height: 100rpx;
  width: 100%;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-shadow: 0 0 8rpx #BFBFBF;
}
.bot_num{
  width: 60%;
  text-align: center;
}
.bot_yy{
  background-color: #2581FF;
  width: 40%;
  height: 100%;
  text-align: center;
  color: #fff;
  border-radius: 0;
  font-size: 32rpx;
}
.bot_cent{
  position: relative;
  top: 10rpx;
}
.con_title{
  text-align: center;
  font-size: 36rpx;
  font-weight: 900;
  color: #5980EA;
  position: relative;
  top:-20rpx;
}
.con_shadow{
  height: 28rpx;
  width: 330rpx;
  background-color: #D6E0FA;
  margin: auto;
  margin-top: 30rpx;
  border-radius: 70rpx;
  position: relative;
  top: 38rpx;
}
.con_xx{
  margin: 30rpx;
  font-size: 30rpx;
  line-height: 45rpx;
}
.con_ys{
  margin: 80rpx 30rpx 0rpx 30rpx;
  line-height: 45rpx;
  font-size: 30rpx;
}
.con_yss{
  font-weight: 550;
}
.con_xj{
  line-height: 45rpx;
  font-size: 30rpx;
  margin: 0rpx 30rpx 30rpx 30rpx;
}